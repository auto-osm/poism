  },
  "homepage": "https://github.com/tmcw/coffeedex",
  "devDependencies": {
    "6to5-browserify": "^1.3.0",
    "browserify": "^6.3.3",
    "minifyify": "^4.4.0",
    "osm-auth": "^0.2.7",
    "react": "^0.12.1",
    "serve": "^1.4.0",
    "watchify": "^2.1.1"
